flow:
  name: "news-world-common-telegraph-rss-spool"

  input:
    plugin: "rss"
    params:
      template: "template.in.world.common.rss.spool"
      input: ["https://www.telegraph.co.uk/news/rss.xml"]

  output:
    plugin: "kafka"
    params:
      template: "template.out.world.common.rss.spool"
      schema:
        lang: "en"
        short_name: "telegraph"
